/* pages/prize/takelist.wxss */
page{
  background:#efefef;
}
.money-wrap{
  margin:0 auto;
  padding:20rpx 2.5%;
  color:#e9e9e9;
}
.money-item{
  margin:0 2%;
}
.money-text{
  
  font-size:32rpx;
  text-align:center;
  line-height:30rpx;
}
.show-money{
  font-size:40rpx;
  line-height:50rpx;
  margin:0 10rpx;
}

/* 列表区域 */
.list-wrap{
  
}
.list-item{
  background:#fff;
}
.list-wrap:last-child{
  margin-bottom:0;
}
.list-head{
  padding:20rpx 2.5% 10rpx 2.5%;
  position:relative;
  height:80rpx;
  line-height:50rpx;
}
.list-title{
  padding:10rpx 2.5% 20rpx 80rpx;
  position:relative;
  height:80rpx;
  line-height:50rpx;
}
.title-img{
  width:55rpx;
  position:absolute;
  margin:auto;
  top:10rpx;
  left:2.5%;
}
.item-title{
  float:left;
  font-size: 30rpx;
  
}
.item-fee{
  float:left;
  color:#666;
}
.item-fee{
  float:right;
  color:#666;
}
.success{
  color:#67b466;
}
.uncheck{
  color:#b3a905;
}
.fail{
  color:#ddd;
}

